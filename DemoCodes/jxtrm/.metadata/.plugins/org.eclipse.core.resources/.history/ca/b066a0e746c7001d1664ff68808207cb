package ExecutorService;

public class Result {

	double answer;
	int lengthOfdigitsAfterPoint;

	public Result(int left, int right) {
		answer = (double)left / right;
		
	}
	public double getAnswer() {
		return answer;
	}

	public int getLengthOfdigitsAfterPoint() {
		return lengthOfdigitsAfterPoint;
	}

}

